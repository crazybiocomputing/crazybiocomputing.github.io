<head>

<script>
var answer = "theanswerisheredonotforgettoaddtheextensionphp";

var alphabet = {
'A':'Ala',
'C':'Cys',
'D':'Asp',
'E':'Glu',
'F':'Phe',
'G':'Gly',
'H':'His',
'I':'Ile',
'K':'Lys',
'L':'Leu',
'M':'Met',
'N':'Asn',
'O':'Pyl', // Pyrrolysine
'P':'Pro',
'Q':'Gln',
'R':'Arg',
'S':'Ser',
'T':'Thr',
'U':'Sec', // Selenocysteine
'V':'Val',
'W':'Trp',
'Y':'Tyr',
'B':'Asx',
'Z':'Glx',
'J':'Xle', // Leucine (L) or Isoleucine (I), used in mass-spec (NMR) 
'X':'Xxx'
}


function main() {
  var str = answer.toUpperCase();
  var out='SEQRES    1 A 60 ';
  var count=2;
  for (var i=0; i< str.length;i++) {
    if (i%13 == 0 && i!= 0) 
      out +='\n'+'SEQRES    '+(count++)+' A 60 ';
    out +=alphabet[str[i]] + ' ';
  }
  console.log(out);
}

</script>

</head>
<body onload="main()">
</body>
